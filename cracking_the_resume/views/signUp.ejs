<!DOCTYPE html>
<html>
<head>
  <script src="https://code.jquery.com/jquery.min.js"></script>
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
  <script src="../javascript/account.js"></script>
  <link rel="stylesheet" type="text/css" href="stylesheets/formpage.css">
</head>
<body>
  <div class="container">
  <div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8 conForm">
            <form name="userData" action = "/split" class= "form-horizontal needs-validation" role="form" method="post">
                <h2>Let's get started</h2>
                <div class="form-group">
                    <label for="firstName" class="col-sm-3 control-label">First Name</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" id="validationCustom01" placeholder="First name" required>
                    </div>
                </div>
                <div class="form-group">
                    <label for="lastName" class="col-sm-3 control-label">Last Name</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" id="validationCustom02" placeholder="Last name" required>
                    </div>
                </div>
                <div class="form-group">
                    <label for="email" class="col-sm-3 control-label">Email</label>
                    <div class="col-sm-9">
                      <input type="email" class="form-control" id="emailAddress" aria-describedby="emailHelp" placeholder="Enter email" required>
                    </div>
                </div>
                <div class="form-group">
                    <label for="userName" class="col-sm-3 control-label">User Name</label>
                    <div class="col-sm-9">
                        <input type="text" name = "userName" id="userName" placeholder="User Name" class="form-control" autofocus required>
                    </div>
                </div>
                <div class="form-group">
                    <label for="password" class="col-sm-3 control-label">Password</label>
                    <div class="col-sm-9">
                      <input type="password" id="inputPassword5" class="form-control" aria-describedby="passwordHelpBlock" placeholder="Password" required>
                      <small id="passwordHelpBlock" class="form-text text-muted">
                        Your password must be 8-20 characters long, contain letters and numbers, and must not contain spaces, special characters, or emoji.
                      </small>                    </div>
                </div>
                <div class="form-group">
                    <label for="country" class="col-sm-3 control-label">Major</label>
                    <div class="col-sm-9">
                        <select id="country" class="form-control" name = "major">
                            <option>Computer Science</option>
                            <option>Computer Engineering</option>
                            <option>Civil Engineering</option>
                            <option>Industrial Engineering</option>
                            <option>Graphic Design</option>
                            <option>Computer Gamer</option>
                            <option>Electrical Engineering</option>
                            <option>Undecided</option>
                        </select>
                    </div>
                </div> <!-- form group -->
                <div class="form-group">
                    <label for="country" class="col-sm-3 control-label">Seeking</label>
                    <div class="col-sm-9">
                        <select id="country" class="form-control" name = "seeking">
                            <option>Internship</option>
                            <option>Full Time</option>
                        </select>
                    </div>
                </div> <!-- form group -->
                <div class="form-group">
                    <label class="control-label col-sm-3">Resume</label>
                    <div class="col-sm-9">
                      <label class="custom-file">
                        <input type="file" class="form-control-file" id="fileUpload" required>
                        <span class="custom-file-control"></span>
                      </label>
                    </div>
                </div> <!-- /.form-group -->
                <div class="form-group">
                    <div class="col-sm-9 col-sm-offset-3">
                        <div class="checkbox">
                            <label>
                                <input type="checkbox" required>I accept <a href="#">terms</a>
                            </label>
                        </div>
                    </div>
                </div> <!-- /.form-group -->
                <div class="form-group">
                    <div class="col-sm-9 col-sm-offset-3">
                      <input type="submit" value="Sign Up"/>
                    </div>
                </div>
            </form> <!-- /form -->
    </div>
    <div class="col-mid-2">
    </div>
  </div>
</div>
